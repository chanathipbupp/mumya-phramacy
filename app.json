{
  "expo": {
    "name": "mumya-pharmacy-app",
    "slug": "mumya-pharmacy-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mumyapharmacyapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Mumya_logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.chanathip.bupp.mumyapharmacyapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "http://72.60.197.70:3000/api/files/view/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2c345aad-d9cb-4cf2-81a5-5778ff81e935"
      }
    }
  }
}
